{
  "cli": {
    "version": ">= 11.0.0"
  },
  "build": {
    "production": {
      "distribution": "store",
      "ios": {
        "resourceClass": "m-medium",
        "autoIncrement": "buildNumber"
      },
      "android": {
        "gradleCommand": ":app:bundleRelease",
        "autoIncrement": "versionCode"
      },
      "env": {
        "EAS_USE_BUN": "0"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {},
      "android": {}
    }
  }
}