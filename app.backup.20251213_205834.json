      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#E75A0B"
      }
    },
    "plugins": [
      "expo-mail-composer",
      "expo-secure-store"
    ],
    "extra": {
